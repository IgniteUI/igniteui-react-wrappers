(function (factory) {
	if (typeof define === "function" && define.amd) {

		// AMD. Register as an anonymous module.
		define( [
			"react",
			"jquery",
            "create-react-class",
            "prop-types"
		], factory );
	} else {

		// Browser globals
		factory(React, jQuery, createReactClass, PropTypes);
	}
}
